// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model obu_invoice {
    obu_id         String @id
    lat            Float
    long           Float
    total_distance Float
    total_price    Float
    created_at     Int
    updated_at     Int
}
